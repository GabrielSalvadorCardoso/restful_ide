<?xml version="1.0" encoding="utf-8"?>
<Map background-color="darkgreen">
    <Style name="point">
        <Rule>
            <MarkersSymbolizer file="marker-icon.png" allow-overlap="yes" />
        </Rule>
    </Style>
    <Style name="multipoint">
        <Rule>
            <MarkersSymbolizer file="marker-icon.png" allow-overlap="yes" />
        </Rule>
    </Style>
    <Style name="line">
        <Rule>
            <LineSymbolizer />
        </Rule>
    </Style>
    <Style name="multilinestring">
        <Rule>
            <LineSymbolizer />
        </Rule>
    </Style>
    <Style name="polygon">
        <Rule>
            <PolygonSymbolizer fill="white" />
        </Rule>
    </Style>
    <Style name="multipolygon">
        <Rule>
            <PolygonSymbolizer fill="#cccccc" />
        </Rule>
    </Style>
</Map>